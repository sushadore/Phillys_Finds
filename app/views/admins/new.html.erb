<h1>Create Admin account</h1>
<div class="row">
  <div class="col-xs-6">
    <%= bootstrap_form_for @admin do |f| %>
      <%= f.text_field :email %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>
      <%= f.submit "Create Admin" %>
    <% end %>
  </div>
</div>
